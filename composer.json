{
    "name": "bobonov/yii2-adminlte3-asset",
    "description": "AdminLte3 theme assets for Yii2 Framework",
    "keywords": ["yii2", "bootstrap4", "adminlte", "adminlte3"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2-bootstrap4/issues",
        "source": "https://github.com/yiisoft/yii2-bootstrap4"
    },
    "authors": [
        {
            "name": "Roberto Braga",
            "email": "bobonov@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "yiisoft/yii2-bootstrap4": "~2.0",
        "almasaeed2010/adminlte":   "~3.0"
    },
    "require-dev": {
        "yiisoft/yii2-coding-standards": "~2.0",
        "phpunit/phpunit": "<7"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "bobonov\\adminlte3\\":"src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
